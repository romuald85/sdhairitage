{% extends "base.html.twig" %}

{% block title %}Éditer la catégorie
	{{category.name}}
{% endblock %}

{% block body %}
	<h1>Éditer la catégorie
		{{category.name}}</h1>

	{{form_start(formView)}}

	{{form_errors(formView)}}

	{{form_row(formView.name)}}

	<button type="submit" class="btn btn-primary">
		<i class="fas fa-save"></i>
		Mettre à jour
	</button>

	{{form_end(formView)}}
{% endblock %}
